{
  "name": "light-generator",
  "version": "1.0.1",
  "description": "light generator for generate a boilerplate",
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "download": "^5.0.2",
    "empty-dir": "^2.0.0",
    "fs-extra": "^8.0.1",
