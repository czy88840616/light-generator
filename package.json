{
  "name": "light-generator",
  "version": "1.7.0",
  "description": "light generator for generate a boilerplate",
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "empty-dir": "^2.0.0",
    "fs-extra": "^8.0.1",
    "light-spinner": "^1.0.0",
    "minimatch": "^3.0.4",
    "tar": "^4.4.10",
    "untildify": "^4.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "midway-bin": "1",
    "tslint": "^5.17.0",
    "tslint-midway-contrib": "1",
    "typescript": "^3.5.1"
  },
  "scripts": {
    "build": "npm run lint && midway-bin build -c",
    "lint": "./node_modules/.bin/tslint --format prose -c ./tslint.json src/**/*.ts test/**/*.ts",
    "test": "npm run lint && cross-env NODE_ENV=test midway-bin test --ts --full-trace",
    "cov": "cross-env NODE_ENV=unittest midway-bin cov --ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "generator",
    "template",
    "boilerplate",
    "tpl",
    "generate",
    "app",
    "tool"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/midwayjs/light-generator.git"
  },
  "author": "czy88840616@gmail.com",
  "license": "MIT"
}
